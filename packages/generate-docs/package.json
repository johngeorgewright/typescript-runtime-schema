{
  "name": "@typescript-runtime-schema/generate-docs",
  "version": "1.0.0",
  "description": "Generate opinionated README.md from source files and/or automatic content",
  "main": "dist/index.js",
  "author": "Simon Johansson <simonljohansson92@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest src/",
    "build": "yarn build:ts",
    "build:ts": "./node_modules/.bin/tsc --project tsconfig.json",
    "build:docs": "node bin/index.js --index-file-path ./docs/index.md -o README.md",
    "prepublish": "yarn build"
  },
  "bin": {
    "generate-docs": "./bin/index.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@types/markdown-table": "^2.0.0",
    "@types/node": "^14.14.10",
    "@types/ramda": "^0.27.32",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@ovyerus/licenses": "^6.4.4",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.1",
    "expand-template": "^2.0.3",
    "globby": "^11.0.1",
    "license": "^1.0.3",
    "markdown-table": "^2.0.0",
    "ramda": "^0.27.1",
    "read-package-json": "^3.0.0",
    "replace-in-file": "^6.1.0",
    "tmp-promise": "^3.0.2",
    "type-fest": "^0.20.2",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.0.11"
  }
}
